{
  "name": "proton-tsc",
  "version": "0.1.3",
  "license": "MIT",
  "main": "./assembly/index.ts",
  "scripts": {
    "build:token": "npx proton-asc ./assembly/token/token.contract.ts",
    "build:balance": "npx proton-asc ./assembly/balance/balance.contract.ts",
    "build:escrow": "npx proton-asc ./assembly/escrow/escrow.contract.ts",
    "build:allow": "npx proton-asc ./assembly/allow/allow.contract.ts",
    "build:kv": "npx proton-asc ./assembly/kv/kv.contract.ts",
    "build:createnft": "npx proton-asc ./assembly/createnft/createnft.contract.ts",
    "build:rng": "npx proton-asc ./assembly/rng/rng.contract.ts",
    "build:txid": "npx proton-asc ./assembly/txid/txid.contract.ts",
    "build:hello": "npx proton-asc ./assembly/hello/hello.contract.ts",
    "build:atomicassets": "npx proton-asc ./assembly/atomicassets/atomicassets.contract.ts --debug --sourceMap --target release",
    "build:store": "npx proton-asc ./assembly/store/store.test.ts",
    "build:safemath": "npx proton-asc ./assembly/safemath/safemath.test.ts",
    "build": "run-p build:*",
    "test:token": "mocha -s 250 ./assembly/token/*.spec.ts -r ts-node/register",
    "test:balance": "mocha -s 250 ./assembly/balance/*.spec.ts -r ts-node/register",
    "test:allow": "mocha -s 250 ./assembly/allow/*.spec.ts -r ts-node/register",
    "test:escrow": "mocha -s 250 ./assembly/escrow/*.spec.ts -r ts-node/register",
    "test:createnft": "mocha -s 250 ./assembly/createnft/*.spec.ts -r ts-node/register",
    "test:kv": "mocha -s 250 ./assembly/kv/*.spec.ts -r ts-node/register",
    "test:rng": "mocha -s 250 ./assembly/rng/*.spec.ts -r ts-node/register",
    "test:txid": "mocha -s 250 ./assembly/txid/*.spec.ts -r ts-node/register",
    "test:hello": "mocha -s 250 ./assembly/hello/*.spec.ts -r ts-node/register",
    "test:atomicassets": "mocha -s 250 ./assembly/atomicassets/*.spec.ts -r ts-node/register",
    "test:store": "mocha -s 250 ./assembly/store/store.spec.ts -r ts-node/register",
    "test:safemath": "mocha -s 250 ./assembly/safemath/safemath.spec.ts -r ts-node/register",
    "test": "run-p test:*",
    "core:escrow": "cat ./assembly/escrow/target/escrow.contract.abi | abi2core > ./assembly/escrow/target/escrow.client-types.ts",
    "run:playground": "LOG_LEVEL=debug npx ts-node ./playground.ts"
  },
  "dependencies": {
    "proton-asc": "^0.2.80"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.0.0"
  },
  "bin": "bin/proton-tsc.js",
  "engineStrict": true,
  "devDependencies": {
    "@greymass/abi2core": "^1.1.0",
    "@greymass/eosio": "^0.5.5",
    "@proton/js": "^26.1.2",
    "@proton/vert": "^0.2.2",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.22",
    "chai": "^4.3.6",
    "cross-fetch": "^3.1.5",
    "crypto": "^1.0.1",
    "mocha": "^9.2.2",
    "npm-run-all": "^4.1.5",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  }
}
